{
  "name": "pixelbeach",
  "productName": "Pixel Beach",
  "version": "1.0.0",
  "private": true,
  "description": "A surfing game",
  "author": "Nathan Hoad",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --runInBand",
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  },
  "contributors": [
    "Nathan Hoad",
    "Lilly Piri",
    "Ben Hoad",
    "Jaap van Hardeveld"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "chance": "^1.0.12",
    "immutable": "^3.8.2",
    "jest": "^21.2.1",
    "phaser-ce": "^2.9.2",
    "uuid": "^3.1.0",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.47.1"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  },
  "build": {
    "appId": "net.nathanhoad.pixelbeach",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": "deb"
    },
    "deb": {
      "compression": "gz"
    }
  }
}
